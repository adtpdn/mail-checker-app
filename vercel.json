{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/routes/apiRoutes.js"
    },
    {
      "src": "(.*)",
      "dest": "/public/index.html"
    }
  ]
}